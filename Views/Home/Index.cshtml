<!-- Top Features -->
<section class="top-features">
    <div class="auto-container">
        <div class="row">


            @foreach (var item in Model.Promo)
            {<div class="feature-block col-lg-4 col-md-6 col-sm-12">
                    <div class="inner-box">
                        <span class="icon @item.Icon"></span>
                        <h4><a href="#">@item.Title</a></h4>
                        <p>@item.Desc</p>
                    </div>
                </div>

            }
            <!-- Feature Block -->
            <!-- Feature Block -->
           
        </div>
    </div>
</section>
<!-- End Features Section -->

<partial name="_clinicalcare" model="Model.ResearchClinic"/>

<!-- Services Section -->
<section class="services-section">
    <div class="auto-container">
        <div class="sec-title text-center">
            <span class="sub-title">Bizim Servislər</span>
            <h2>Xəstələrimizə daim qayğı göstəririk</h2>
            <span class="divider"></span>
        </div>


        <partial name="_services" model="Model.DiaqnosticServis"/>



    </div>
</section>
<!--End Services Section -->


<partial name="_doctorteam" model="Model.Doctors"/>


<partial name="_appoinment" model="Model.GetAppointment"/>


<partial name="_patientsay" model="Model.PatientSays"/>


<!-- Pricing Section -->
<section class="pricing-section">
    <div class="auto-container">
        <div class="sec-title text-center">
            <span class="sub-title">Check-Up Paketlərimiz</span>
            <h2>Qiymət Planı</h2>
            <span class="divider"></span>
        </div>

        <div class="outer-box">
            <div class="row">

                @foreach (var item in Model.Checkups)
                {<div class="pricing-block col-lg-3 col-md-6 col-sm-12">

    <div class="inner-box">
        <div class="price-box">
            <h4 class="price">@item.Price  ₼</h4>
            <div class="validaty">Hər ay üçün</div>
        </div>
        <h3 class="title">@item.Name</h3>
        <ul class="features">

            @foreach (var items in item.CheckupSettings)
            {<li>@items.Name</li>

            }
            
           
        </ul>

    </div>
</div>

                }
               
            </div>
        </div>

        <div class="sec-bottom-text">Bütün Dr. Komandasını araşdırın<a href="#">Komandamızdan Cavabdehik.</a></div>
    </div>
</section>
<!--End Pricing Section -->
<!-- News Section -->
<section class="news-section">
    <div class="auto-container">
        <!-- Sec Title -->
        <div class="sec-title text-center">
            <span class="title">Blogumuz</span>
            <h2>Son məqalələr və xəbərlər</h2>
            <span class="divider"></span>
        </div>

        <div class="row">
            @foreach (var item in Model.News)
            {<div class="news-block col-lg-4 col-md-6 col-sm-12 wow fadeInUp">
                    <div class="inner-box">
                        <div class="image-box">
                            <figure class="image"><a asp-controller="Home" asp-action="NewsReadMore" asp-route-id="@item.Id"><img src="~/Uploads/gallery/@item.Photo" alt=""></a></figure>
                            <a asp-controller="Home" asp-action="NewsReadMore" asp-route-id="@item.Id" class="date">@item.Date</a>
                        </div>
                        <div class="lower-content">
                            <h4><a asp-controller="Home" asp-action="NewsReadMore" asp-route-id="@item.Id">@item.Title</a></h4>
                            @*<div class="text">@item.Desc</div>*@

                        </div>
                    </div>
                </div>

            }
            <!-- News Block -->
            

        </div>
    </div>
</section>
<!--End News Section -->

<partial name="_clients" model="Model.Clients"/>
