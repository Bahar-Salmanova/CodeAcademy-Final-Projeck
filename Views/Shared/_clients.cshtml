@model List< CodeClinic.Models.Clients>



<!-- Clients Section -->
<section class="clients-section">
    <div class="auto-container">

        <!-- Sponsors Outer -->
        <div class="sponsors-outer">
            <!--clients carousel-->
            <ul class="clients-carousel owl-carousel owl-theme">
                @foreach (var item in Model)
                {<li class="slide-item"> <a href="@item.Url"><img src="~/Uploads/gallery/@item.Photo" alt=""></a> </li>

                }
                
                
            </ul>
        </div>
    </div>
</section>
<!--End Clients Section -->